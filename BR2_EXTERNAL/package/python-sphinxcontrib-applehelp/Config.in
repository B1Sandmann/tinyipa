# vim: noexpandtab filetype=make
config BR2_PACKAGE_PYTHON_PYTHON_SPHINXCONTRIB_APPLEHELP
	bool "python-sphinxcontrib-applehelp"
	depends on BR2_PACKAGE_PYTHON3
	select BR2_PACKAGE_PYTHON_FLAKE8
	select BR2_PACKAGE_PYTHON_MYPY
	select BR2_PACKAGE_PYTHON_DOCUTILS_STUBS
	select BR2_PACKAGE_PYTHON_PYTEST
	help
	  sphinxcontrib-applehelp is a sphinx extension which outputs Apple help books

	  http://sphinx-doc.org/
